{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    // "baseUrl": "./",
    "paths": {
      "@components/*": ["./components/*"],
      "@styles/*": ["./styles/*"],
      "@contexts/*": ["./contexts/*"],
      "@utils/*": ["./utils/*"],
      "@api/*": ["./api/*"]
    },
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "../../@types/*.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
